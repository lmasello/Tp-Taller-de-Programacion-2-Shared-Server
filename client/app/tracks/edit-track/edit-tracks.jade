.edit-track
    h2 Editar canción
    form(action='')
        p Agregar
        input(type='name', placeholder='Nombre', ng-model='$ctrl.track.name')
        input(type='duration', placeholder='Duración', ng-model='$ctrl.track.duration')
        input(type='button', value='Editar', ng-click='$ctrl.editTrack()')